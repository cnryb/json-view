{
  "manifest_version": 3,
  "name": "JSON-View",
  "version": "0.0.1.1",
  "description": "json view",
  "author": "cnryb@live.cn",
  "homepage_url": "https://github.com/cnryb/json-view",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "node_modules/*",
        "index.*"
      ]
    }
  ]
}